{
    "extends": [
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": ["@typescript-eslint"],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "@typescript-eslint/no-explicit-any": "off"  // Disable the rule that checks for 'any'
    },
    "env": {
      "node": true,
      "browser": true
    }
  }
  